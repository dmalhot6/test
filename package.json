{
  "name": "@amzn/code-pipeline-console-assets",
  "version": "0.0.1",
  "description": "CodePipeline console assets",
  "engines": {
    "node": ">=18.x"
  },
  "npm-pretty-much": {
    "runTest": "never",
    "runRelease": "release"
  },
  "browserslist": [
    "extends @amzn/browserslist-config-aws-console"
  ],
  "scripts": {
    "build": "webpack --mode production && rsync -va configuration/ build/",
    "build+test": "npm run build && npm run test",
    "clean": "rm -rf node_modules public build",
    "compress-artifacts": "node ./scripts/zipArtifacts.js",
    "copy:bluefox": "dir=build/bluefox.d/CodePipelineConsoleAssets; mkdir -p $dir && cp bluefox-config.json package.json package-lock.json $dir/",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,jsx,scss,json}' --check",
    "format": "prettier '**/*.{ts,tsx,js,jsx,scss,json}' --list-different",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint": "eslint --ext .ts,.tsx",
    "pretest": "npm run format && npm run lint && npm run security",
    "release": "npm run build && npm run copy:bluefox",
    "security": "eslint . --ext .ts,.tsx -c .eslintrc-security.js --no-eslintrc",
    "server": "npm run start",
    "start": "webpack serve --mode development",
    "syncLoc:debug": "LOG_LEVEL=4 node ./scripts/syncLocalization.js",
    "syncLoc": "LOG_LEVEL=3 node ./scripts/syncLocalization.js",
    "test": "npm run lint && npm run security && npm run test+only",
    "test+only": "TZ=Canada/Pacific NODE_ENV=test jest --config jest.config.js",
    "test+only+watch": "TZ=Canada/Pacific NODE_ENV=test jest --config jest.config.js --watch",
    "typecheck:watch": "tsc --noEmit --watch",
    "typecheck": "tsc --noEmit",
    "update-snapshots": "TZ=Canada/Pacific NODE_ENV=test jest --config jest.config.js --updateSnapshot",
    "prepare": "husky install"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./build/brazil-unit-tests/TESTS-TestSuites.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  },
  "repository": {
    "type": "git",
